{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "gyex",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "open": true
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "bundle": {
      "identifier": "studio.tauri.dev.gyex",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "windows": [
      {
        "minHeight": 500,
        "minWidth": 400,
        "fullscreen": false,
        "resizable": true,
        "title": "Gyex",
        "center": true,
        "height": 500,
        "width": 400
      }
    ]
  }
}
